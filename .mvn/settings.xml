            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>maven.org</id>
                    <name>maven.org</name>
                    <url>http://repo1.maven.org/maven2</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
